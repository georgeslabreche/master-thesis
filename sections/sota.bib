  file      = {:E\:\\doc\\literature\\references\\suspensionSystems_roverSystems\\MER_2005_mobility_assembly_design_test.pdf:PDF},
  keywords  = {Mars;aerospace engineering;aerospace robotics;mobile robots;planetary rovers;remotely operated vehicles;space vehicles;AD 2004 01;Mars Exploration Rover;Mars terrain;Martian surface;NASA;Opportunity;Spirit;mobile robotic spacecraft;planetary exploration;robotic geologist;rover mobility assembly design;Geology;Marine vehicles;Mars;Mobile robots;NASA;Orbital robotics;Planets;Robotic assembly;Space vehicles;Testing;Mars;Rover;mobility;rocker bogie suspension;traversability},
  owner     = {cordes},
