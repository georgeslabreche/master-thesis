@InProceedings{Krotkov91-ActivePerceptionOfMaterialAndShape,
  author    = {Krotkov, E.},
  title     = {Active perception of material and shape by a walking robot},
  booktitle = { 1991 IEEE International Conference on Advanced Robotics (ICAR'91)},
  year      = {1991},
  pages     = {37-42 vol.1},
  month     = {Jun},
