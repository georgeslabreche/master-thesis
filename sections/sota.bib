@Article{Cao1997,
  author   = {Cao, Y. Uny and Fukunaga, Alex S. and Kahng, Andrew},
  title    = {Cooperative Mobile Robotics: Antecedents and Directions},
  journal  = {Autonomous Robots},
  year     = {1997},
  volume   = {4},
  number   = {1},
  pages    = {7--27},
  month    = {Mar},
  issn     = {1573-7527},
  abstract = {There has been increased research interest in systems composed of multiple autonomous mobile robots exhibiting cooperative behavior. Groups of mobile robots are constructed, with an aim to studying such issues as group architecture, resource conflict, origin of cooperation, learning, and geometric problems. As yet, few applications of cooperative robotics have been reported, and supporting theory is still in its formative stages. In this paper, we give a critical survey of existing works and discuss open problems in this field, emphasizing the various theoretical issues that arise in the study of cooperative robotics. We describe the intellectual heritages that have guided early research, as well as possible additions to the set of existing motivations.},
  day      = {01},
  doi      = {10.1023/A:1008855018923},
  url      = {https://doi.org/10.1023/A:1008855018923},
}

@InProceedings{Brown1995,
  author    = {R. G. Brown and J. S. Jennings},
  title     = {A pusher/steerer model for strongly cooperative mobile robot manipulation},
  booktitle = {1995 IEEE/RSJ International Conference on Intelligent Robots and Systems (IROS'95)},
  year      = {1995},
  volume    = {3},
  pages     = {562-568 vol.3},
  month     = {Aug},
  abstract  = {Presents an empirical example of a strongly cooperative manipulation system (the pusher/steerer system), whose capabilities derive from the redistribution of resources typically used in a single robot system. The authors define strongly cooperative strategies to be, intuitively, those which are not trivially serializable. In the authors' system, one robot steers, and the other pushes; the object lies between them. The steerer is the only agent that has information about the path, while the pusher exerts the necessary motive force, and rotates to follow changes in the object's orientation. The system is asynchronous, with no explicit communication between the robots. The authors are interested in studying manipulation systems of this type, because they are strongly cooperative; that is, the two robots must act in concert to achieve the goal. The authors have performed over one hundred pusher/steerer manipulation experiments with two of their mobile robots, TOMMY and LILY. As the authors' analysis predicts, they have found that their system allows a wide variety of paths and manipulable objects for these robots},
  doi       = {10.1109/IROS.1995.525941},
  keywords  = {cooperative systems;manipulators;mobile robots;path planning;LILY;TOMMY;asynchronous system;pusher/steerer model;strongly cooperative mobile robot manipulation;Computer science;Friction;Kinematics;Large-scale systems;Manipulator dynamics;Mobile robots;Multirobot systems;Navigation;Parallel robots;Robot programming},
}

