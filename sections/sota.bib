@InProceedings{Reid2014,
  author    = {W. Reid AND A. H. G\"oktogan AND S. Sukkarieh},
  title     = {Moving MAMMOTH: Stable Motion for a Reconfigurable Wheel-On-Leg Rover},
  booktitle = {2014 Australasian Conference on Robotics and Automation},
  year      = {2014},
  abstract  = {Reconfigurable wheel-on-leg rovers allow for a
variety of locomotion modes and possible insta-
bility as a result of configuration changes. Pre-
sented is a kinematic model for the actuation
of a wheel-on-leg platform along with a tip-over
stability model. The kinematic model allows
