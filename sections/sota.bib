  keywords = {mobile robots;multi-agent systems;multi-robot systems;pattern classification;artificial intelligence;coordination;multirobot systems;robotics;system classification;Hardware;Intelligent robots;Multirobot systems;Reliability engineering;Robot kinematics;Robot sensing systems;Sensor phenomena and characterization;Sensor systems;Switches;Taxonomy;Algorithms;Artificial Intelligence;Computer Communication Networks;Models, Theoretical;Motion;Robotics;Systems Integration},
